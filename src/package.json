{
  "name": "swole-experience-react",
  "version": "0.1.0",
  "private": true,
  "description": "SwoleExperience React Native (Expo) app with web support",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "prepare": "husky install || true"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/material": "^7.3.6",
    "@mui/x-date-pickers": "^8.22.0",
    "@react-native-async-storage/async-storage": "2.2.0",
    "date-fns": "^4.1.0",
    "expo": "~54.0.12",
    "expo-router": "~6.0.10",
    "expo-status-bar": "~3.0.8",
    "react": "19.1.0",
    "react-calendar": "^6.0.0",
    "react-date-picker": "^12.0.1",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "^2.28.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-uuid": "^2.0.3",
    "react-native-vector-icons": "^10.0.3",
    "react-native-web": "^0.21.0",
    "victory": "^37.3.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/react-native": "^12.7.2",
    "@types/jest": "^29.5.12",
    "@types/react": "~19.1.10",
    "@types/react-calendar": "^4.1.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.37.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.1.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-expo": "~52.0.1",
    "react-test-renderer": "19.1.0",
    "typescript": "~5.9.2"
  },
  "jest": {
    "preset": "react-native",
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|@react-native|@react-native-async-storage|react-native-uuid|victory|@testing-library|@babel|expo|expo-router|expo-status-bar|react-native-gesture-handler|react-native-safe-area-context|react-native-screens|react-native-svg|react-native-vector-icons|react-native-web|@mui|@emotion|date-fns)/)"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js",
      "<rootDir>/jest.setup.rntl.js"
    ],
    "testMatch": [
      "**/__tests__/**/*.(test|spec).(js|jsx|ts|tsx)"
    ],
    "collectCoverageFrom": [
      "lib/**/*.{js,jsx,ts,tsx}",
      "hooks/**/*.{js,jsx,ts,tsx}",
      "!**/*.d.ts",
      "!**/node_modules/**"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "^@react-native-async-storage/async-storage$": "<rootDir>/node_modules/@react-native-async-storage/async-storage"
    }
  }
}
